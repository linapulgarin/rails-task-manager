<h1>Edit task</h1>

<div class="form-control">
  <%= form_for(@task) do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :details %>
    <%= f.text_field :details %>
    <%= f.label :completed %>
    <%= f.check_box :completed %>
    <%= f.submit %>
  <%end%>
</div>
<br>
<%= link_to "Return task", tasks_path %>
